{
  "build": {
    "distDir": "../static",
    "devPath": "http://http://localhost:3000/?pls"
  },
  "ctx": {},
  "tauri": {
    "embeddedServer": {
      "active": true
    },
    "bundle": {
      "active": true,
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "targets": [
        "deb",
        "osx",
        "msi",
        "appimage"
      ]
    },
    "whitelist": {
      "all": false
    },
    "window": {
      "title": "Tauri App Hello World"
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    },
    "edge": {
      "active": true
    },
    "inliner": {
      "active": true
    }
  }
}